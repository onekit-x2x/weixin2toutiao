<import src="../../../common/head.ttml"></import>
<import src="../../../common/foot.ttml"></import>

<view  class="container">
  <template  is="head" data="{{title: 'image'}}"></template>

  <view  class="page-body">
    <view  class="page-section page-section-gap">
      <view  class="page-section-title">Local Image</view>
      <view  class="page-section-ctn">
        <image  class="image" src="../../resources/pic/1.jpg"></image>
      </view>
    </view>
    <view  class="page-section page-section-gap">
      <view  class="page-section-title">Internet Image</view>
      <view  class="page-section-ctn">
        <image  class="image" src="{{imageUrl}}"></image>
      </view>
    </view>
  </view>

  <template  is="foot"></template>
</view>
