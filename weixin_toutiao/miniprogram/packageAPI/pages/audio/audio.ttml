<view>
  <audio src="{{src}}" id="myAudio"></audio>

  <onekit-button type="primary" bindtap="audioPlay">播放</onekit-button>
  <onekit-button type="primary" bindtap="audioPause">暂停</onekit-button>
  <onekit-button type="primary" bindtap="audio14">设置当前播放时间为14秒</onekit-button>
  <onekit-button type="primary" bindtap="audioStart">回到开头</onekit-button>
</view>